# Example Environment Variables for Env Stamp
# Copy to .env.local for local development or set in deployment platform

# Environment identifier (local, dev, test, prod)
VITE_APP_ENV=local

# Application name
VITE_APP_NAME=Env Stamp

# Debug mode (true/false)
VITE_DEBUG_MODE=true

# Analytics enabled (true/false)
VITE_ANALYTICS_ENABLED=false

# Supabase integration (true/false)
VITE_SUPABASE_ENABLED=false